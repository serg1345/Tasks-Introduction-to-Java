package by.jonline.modul1.task1;

//решить уравнение z = ((a-3)*b/2)+c

public class Task1 {
	public static void main (String[] args) {
		
		double a;
		double b;
		double c;
		double result;
		
		a = 3.0;
		b = 7.2;
		c = 8.5;
		
		result = ((a-3)*b/2)+c;
		System.out.println("Результат: "+result);
		
	}
}
